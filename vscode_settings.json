{
    "explorer.confirmDragAndDrop": false,
    "http.proxyStrictSSL": false,
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },
    "vim.leader": " ",
    "vim.highlightedyank.enable": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    // "vim.autoSwitchInputMethod.enable": true,
    // "vim.autoSwitchInputMethod.defaultIM": "1033",
    // "vim.autoSwitchInputMethod.obtainIMCmd": "D:\\bin\\im-select.exe",
    // "vim.autoSwitchInputMethod.switchIMCmd": "D:\\bin\\im-select.exe {im}",
    "vim.smartRelativeLine": true,
    "vim.langmap": "ФИСВУАПРШОЛДЬТЩЗЙКЫЕГМЦЧНЯ;ABCDEFGHIJKLMNOPQRSTUVWXYZ,фисвуапршолдьтщзйкыегмцчня;abcdefghijklmnopqrstuvwxyz",
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "."
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "c"
            ],
            "commands": [
                "editor.action.commentLine",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "P"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "t",
                "i"
            ],
            "commands": [
                "java.test.goToTest"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "g"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        },
        {
            "before": [
                "<S-k>"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "u"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "o"
            ],
            "commands": [
                "editor.action.organizeImports"
            ]
        },
        {
            "before": [
                "<leader>",
                "."
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<leader>",
                "["
            ],
            "commands": [
                "editor.action.marker.prev"
            ],
        },
        {
            "before": [
                "<leader>",
                "]"
            ],
            "commands": [
                "editor.action.marker.next"
            ],
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "g"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "<leader>",
                "z",
                "z"
            ],
            "commands": [
                "workbench.action.toggleZenMode"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "i"
            ],
            "commands": [
                "toggleVim",
                "editor.action.goToImplementation",
                "toggleVim",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "d"
            ],
            "commands": [
                "toggleVim",
                "editor.action.revealDefinition",
                "toggleVim",
                "extension.vim_escape"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "q"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        {
            "before": [
                "leader",
                "g",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "workbench.explorer.fileView.focus"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateBack"
                }
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                {
                    "command": "workbench.action.navigateForward"
                }
            ]
        }
    ],
    "files.associations": {
        "*.ajs": "javascript"
    },
    "background.windowBackgrounds": [
        "C:/Users/StepanovSA/Pictures/wallpapers/crusade.jpg"
    ],
    "background.autoInstall": true,
    "extensions.autoUpdate": false,
    "update.mode": "manual",
    "extensions.autoCheckUpdates": false,
    "workbench.remoteIndicator.showExtensionRecommendations": false,
    "yaml.extension.recommendations": false,
    "extensions.ignoreRecommendations": true,
    "python.testing.pytestEnabled": true,
    "hediet.vscode-drawio.resizeImages": null,
    "cSpell.language": "en-GB,ru-RU",
    "cSpell.languageSettings": [],
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "cSpell.userWords": [
    ],
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "explorer.confirmDelete": false,
    "continue.telemetryEnabled": false,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "workbench.colorTheme": "Github Light High Contrast"
}
